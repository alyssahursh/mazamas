<p id="notice"><%= notice %></p>

<h1>Listing Climber Profiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @climber_profiles.each do |climber_profile| %>
      <tr>
        <td><%= link_to 'Show', climber_profile %></td>
        <td><%= link_to 'Edit', edit_climber_profile_path(climber_profile) %></td>
        <td><%= link_to 'Destroy', climber_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Climber profile', new_climber_profile_path %>
