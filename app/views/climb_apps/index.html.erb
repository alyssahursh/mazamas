<p id="notice"><%= notice %></p>

<h1>Listing ClimbApps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @climb_apps.each do |climb_app| %>
      <tr>
        <td><%= link_to 'Show', climb_app %></td>
        <td><%= link_to 'Edit', edit_climb_app_path(climb_app) %></td>
        <td><%= link_to 'Destroy', climb_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New ClimbApp', new_climb_app_path %>
