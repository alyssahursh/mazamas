<h4 id="notice" class="f3"><%= notice %></h4>

<h1>Listing ClimbApps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @climb_apps.each do |climb_app| %>
      <tr>
        <td><%= climb_app.climb.mountain.name %></td>
        <td><%= climb_app.id  %></td>
        <td><%= climb_app.climb_app_status  %></td>
        <td><%= link_to "Edit", edit_user_climb_app_path(current_user, climb_app) %></td>
        <td><%= link_to 'Destroy', user_climb_app_path(current_user, climb_app), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
