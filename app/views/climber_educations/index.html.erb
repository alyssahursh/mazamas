<p id="notice"><%= notice %></p>

<h1>Listing Climber Educations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @climber_educations.each do |climber_education| %>
      <tr>
        <td><%= link_to 'Show', climber_education %></td>
        <td><%= link_to 'Edit', edit_climber_education_path(climber_education) %></td>
        <td><%= link_to 'Destroy', climber_education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

